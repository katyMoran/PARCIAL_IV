            <label>Asigne la nota 1</label><br />
            <input id="N1" type="number" required name="N1" min="1" max="5" /> <br />
            <label>Asigne la nota 2</label><br />
            <input id="N1" type="number" required name="N2" min="1" max="5" /> <br />
            <label>Asigne la nota 3</label><br />
